class Solution {
public:
    int findMaxK(vector<int>& nums) {
        sort(nums.begin(), nums.end());
        int l = 0;
        int r = nums.size() - 1;
        while (l < r) {
            if (abs(nums[l]) == nums[r]) {
                return nums[r];
            }
            if (abs(nums[l]) > nums[r]) {
                l++;
            } else {
                r--;
            }
        }
        return -1;
    }
};
